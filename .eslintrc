{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "generators": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-multi-spaces": 1,
    "key-spacing": 1,
    "no-multiple-empty-lines": 1,
    "no-trailing-spaces": 1,
    "eol-last": 1,
    "space-before-blocks": 1,
    "padded-blocks": 1,
    "no-shadow": 1,
    "prefer-const": 1,
    "no-loop-func": 0,
    "no-use-before-define": 1,
    "no-param-reassign": 1,
    "semi": 0,
    "no-empty-label": 0,
    "no-labels": 0,
    "comma-dangle": 0,
    "jsx-quotes": 0,
    "object-curly-spacing": 0,
    "no-console": 0,
    "no-constant-condition": 0,
    "spaced-comment": 0,
    "new-cap": 0,
    "max-len": 0,
    // React
    "react/prop-types": 0,
    "react/no-multi-comp": 0,
    "react/jsx-no-bind": 0
  }
}
